{
  "name": "@papillonads/styleguide",
  "version": "1.15.0",
  "description": "Styleguide Microsite",
  "homepage": "https://github.com/papillonads/papillonads/tree/master/packages/styleguide",
  "repository": {
    "type": "git",
    "url": "https://github.com/papillonads/papillonads.git",
    "directory": "packages/styleguide"
  },
  "files": [
    "build",
    "README.md"
  ],
  "keywords": [
    "styleguide microsite"
  ],
  "author": {
    "name": "MTS",
    "url": "https://github.com/mts"
  },
  "scripts": {
    "clean-up": "rm -rf build node_modules package-lock.json",
    "install-packages": "npm install",
    "install-papillonads-latest": "echo \"styleguide skipping install-papillonads-latest\" && exit 0",
    "make-404-page": "cp build/index.html build/404.html",
    "build-test": "webpack --config webpack.test.js --progress --hide-modules && npm run make-404-page",
    "build-acceptance": "webpack --config webpack.acceptance.js --progress --hide-modules && npm run make-404-page",
    "build-release": "webpack --config webpack.release.js --progress --hide-modules && npm run make-404-page",
    "start": "nodemon --watch webpack.settings.js --watch webpack.common.js --watch webpack.develop.js --exec \"webpack-dev-server --config webpack.develop.js\""
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR"
    ],
    "legacyBrowsers": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR"
    ],
    "modernBrowsers": [
      "last 2 Chrome versions",
      "not Chrome < 60",
      "last 2 Safari versions",
      "not Safari < 10.1",
      "last 2 iOS versions",
      "not iOS < 10.3",
      "last 2 Firefox versions",
      "not Firefox < 54",
      "last 2 Edge versions",
      "not Edge < 15"
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "webpack": "^4.46.0",
    "webpack-dev-server": "^3.11.3"
  }
}
